CC = gcc -W -Wall
CHAP = 5

test1: Hat$(CHAP)-1.o
	printf "[hat$(CHAP)-1]\n" >> test.out
	./$< >> test.out

test2-1: Hat$(CHAP)-2-1.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-2-1]\n" >> test.out
	./$< >> test.out

test2-2: Hat$(CHAP)-2-2.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-2-2]\n" >> test.out
	./$< >> test.out

test2-3: Hat$(CHAP)-2-3.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-2-3]\n" >> test.out
	./$< >> test.out

test2-4: Hat$(CHAP)-2-4.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-2-3]\n" >> test.out
	./$< >> test.out

test3-1: Hat$(CHAP)-3-1.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-3-1]\n" >> test.out
	./$< >> test.out

test3-2: Hat$(CHAP)-3-2.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-3-2]\n" >> test.out
	./$< >> test.out

test3-3: Hat$(CHAP)-3-3.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-3-3]\n" >> test.out
	./$< >> test.out

test3-4: Hat$(CHAP)-3-4.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-3-4]\n" >> test.out
	./$< >> test.out

test4: Hat$(CHAP)-4.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-4]\n" >> test.out
	printf "1\n50\n33\n" | ./$< >> test.out

test5: Hat$(CHAP)-5.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-5]\n" >> test.out
	./$< >> test.out

test6: Hat$(CHAP)-6.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-6]\n" >> test.out
	printf "33\n" | ./$< >> test.out

test7: Hat$(CHAP)-7.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-7]\n" >> test.out
	printf "28\n" | ./$< >> test.out

test8: Hat$(CHAP)-8.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-8]\n" >> test.out
	printf "3\n31\n" | ./$< >> test.out

test9: Hat$(CHAP)-9.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-9]\n" >> test.out
	printf "3\n"./$< >> test.out

test10: Hat$(CHAP)-10.o
	printf "\n" >> test.out
	printf "[hat$(CHAP)-10]\n" >> test.out
	printf "1234\n3567\n" | ./$< >> test.out


%.o: %.c
	$(CC) $< -o $@

clean:
	rm -rf *.o *.out
