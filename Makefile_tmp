test10: Hat5-10.o
	printf "\n" >> test.out
	printf "[hat5-10]\n" >> test.out
	printf "1234\n3567\n" | ./$< >> test.out


%.o: %.c
	$(CC) $< -o $@

clean:
	rm -rf *.o *.out
